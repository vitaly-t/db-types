{
  "name": "db-types",
  "version": "0.0.6",
  "description": "Universal database type mapper",
  "main": "lib/index.js",
  "scripts": {
    "test": "jasmine-node test",
    "doc": "./node_modules/.bin/jsdoc -c ./jsdoc/jsDoc.json ./jsdoc/README.md",
